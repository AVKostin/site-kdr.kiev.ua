447\\u0430\\u044f\\u043c\\u0438, \\u043a\\u043e\\u0433\\u0434\\u0430 \\u043d\\u0435 \\u0447\\u0438\\u0442\\u0430\\u0435\\u0442\\u0441\\u044f \\u0444\\u043b\\u0435\\u0448\\u043a\\u0430 transcend \\u0438\\u043b\\u0438 \\u043d\\u0430\\u0447\\u0438\\u043d\\u0430\\u0435\\u0442 \\u0432\\u044b\\u0434\\u0430\\u0432\\u0430\\u0442\\u044c \\u043e\\u0448\\u0438\\u0431\\u043a\\u0438 \\u0440\\u0430\\u0437\\u043d\\u043e\\u0433\\u043e \\u0440\\u043e\\u0434\\u0430. \\u0412 \\u0442\\u0430\\u043a\\u0438\\u0445 \\u0441\\u043b\\u0443\\u0447\\u0430\\u044f\\u0445 \\u043c\\u044b \\u0440\\u0435\\u043a\\u043e\\u043c\\u0435\\u043d\\u0434\\u0443\\u0435\\u043c \\u2013 \\u0411\\u0415\\u0417 \\u041f\\u0410\\u041d\\u0418\\u041a\\u0418. \\u041d\\u0435\\u0442 \\u0431\\u0435\\u0437\\u043d\\u0430\\u0434\\u0435\\u0436\\u043d\\u044b\\u0445 \\u0441\\u043b\\u0443\\u0447\\u0430\\u0435\\u0432 \\u2013 \\u0433\\u043b\\u0430\\u0432\\u043d\\u043e\\u0435 \\u0437\\u043d\\u0430\\u0442\\u044c \\u043a \\u043a\\u043e\\u043c\\u0443 \\u043e\\u0431\\u0440\\u0430\\u0442\\u0438\\u0442\\u0441\\u044f \\u0437\\u0430 \\u043f\\u043e\\u043c\\u043e\\u0449\\u044c\\u044e. \\u0423 \\u043d\\u0430\\u0441 \\u0412\\u044b \\u043c\\u043e\\u0436\\u0435\\u0442\\u0435 \\u043e\\u0441\\u0442\\u0430\\u0432\\u0438\\u0442\\u044c \\u043d\\u0430 \\u0412\\u043e\\u0441\\u0441\\u0442\\u0430\\u043d\\u043e\\u0432\\u043b\\u0435\\u043d\\u0438\\u0435 \\u0444\\u043b\\u0435\\u0448\\u043a\\u0438 Transcend (\\u043d\\u0435 \\u0440\\u0435\\u043c\\u043e\\u043d\\u0442, \\u0430 \\u0438\\u043c\\u0435\\u043d\\u043d\\u043e \\u0432\\u043e\\u0441\\u0441\\u0442\\u0430\\u043d\\u043e\\u0432\\u043b\\u0435\\u043d\\u0438\\u0435 \\u0434\\u0430\\u043d\\u043d\\u044b\\u0445) \\u0438 \\u0431\\u044b\\u0442\\u044c \\u0443\\u0432\\u0435\\u0440\\u0435\\u043d\\u043d\\u044b\\u043c, \\u0447\\u0442\\u043e \\u043d\\u0430\\u0448\\u0438 \\u0441\\u043f\\u0435\\u0446\\u0438\\u0430\\u043b\\u0438\\u0441\\u0442\\u044b \\u0441\\u0434\\u0435\\u043b\\u0430\\u044e\\u0442 \\u0432\\u0441\\u0435, \\u04