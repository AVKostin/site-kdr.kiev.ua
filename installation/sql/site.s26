 \\u0432\\u0456\\u0434\\u043d\\u043e\\u0432\\u043b\\u044e\\u0432\\u0430\\u0442\\u0438 \\u0434\\u0430\\u043d\\u0456 \\u0437 \\u0444\\u043b\\u0435\\u0448\\u043e\\u043a \\u0430\\u0431\\u043e \\u043a\\u0430\\u0440\\u0442 \\u043f\\u0430\\u043c\'\\u044f\\u0442\\u0456, \\u0430\\u043b\\u0435 \\u043f\\u043e \\u0444\\u0430\\u043a\\u0442\\u0443 - 90%, \\u0449\\u043e \\u0444\\u0456\\u0440\\u043c\\u0430-\\u043f\\u043e\\u0441\\u0435\\u0440\\u0435\\u0434\\u043d\\u0438\\u043a \\u043c\\u0456\\u0436 \\u043b\\u0430\\u0431\\u043e\\u0440\\u0430\\u0442\\u043e\\u0440\\u0456\\u0454\\u044e \\u0442\\u0430 \\u0412\\u0430\\u043c\\u0438 (\\u043a\\u043b\\u0456\\u0454\\u043d\\u0442\\u043e\\u043c), \\u0449\\u043e \\u0432 \\u043f\\u0456\\u0434\\u0441\\u0443\\u043c\\u043a\\u0443 \\u043f\\u043e\\u0437\\u043d\\u0430\\u0447\\u0430\\u0454\\u0442\\u044c\\u0441\\u044f \\u043d\\u0430 \\u0448\\u0432\\u0438\\u0434\\u043a\\u043e\\u0441\\u0442\\u0456 \\u0432\\u0438\\u043a\\u043e\\u043d\\u0430\\u043d\\u043d\\u044f \\u0440\\u043e\\u0431\\u0456\\u0442 \\u0456 \\u043d\\u0430 \\u043a\\u0456\\u043d\\u0446\\u0435\\u0432\\u0456\\u0439 \\u0432\\u0430\\u0440\\u0442\\u043e\\u0441\\u0442\\u0456. Kiev Data Recovery - \\u0446\\u0435 \\u0441\\u0430\\u043c\\u0435 \\u043b\\u0430\\u0431\\u043e\\u0440\\u0430\\u0442\\u043e\\u0440\\u0456\\u044f, \\u0434\\u0435 \\u0440\\u0435\\u0430\\u043b\\u044c\\u043d\\u043e \\u0454 \\u043d\\u0435\\u043e\\u0431\\u0445\\u0456\\u0434\\u043d\\u0435 \\u043e\\u0431\\u043b\\u0430\\u0434\\u043d\\u0430\\u043d\\u043d\\u044f \\u0456 \\u043f\\u0440\\u0430\\u0446\\u044e\\u044e\\u0442\\u044c \\u043a\\u0432\\u0430\\u043b\\u0456\\u0444\\u0456\\u043a\\u043e\\u0432\\u0430\\u043d\\u0456 \\u0444\\u0430\\u0445\\u0456\\u0432\\u0446\\u0456. \\u0427\\u0430\\u0441\\u0442\\u043e \\u0431\\u0443\\u0432\\u0430\\u0454 \\u0442\\u0430\\u043a, \\u0444\\u043b\\u0435\\u0448\\u043a\\u0430 Kingston \\u043d\\u0435 \\u043f\\u0440\\u0430\\u0446\\u044e\\u0454 \\u0456 \\u043f\\u0435\\u0440\\u0435\\u0441\\u0442\\u0430\\u043b\\u0430 \\u04