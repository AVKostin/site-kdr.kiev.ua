\\u044f \\u0432 2009 \\u0440\\u043e\\u043a\\u0443, \\u043a\\u043e\\u043b\\u0438 \\u043f\\u043e\\u0442\\u0440\\u0456\\u0431\\u043d\\u043e \\u0431\\u0443\\u043b\\u043e \\u0432\\u0456\\u0434\\u043d\\u043e\\u0432\\u0438\\u0442\\u0438 \\u0434\\u0430\\u043d\\u0456 \\u0437 \\u043d\\u0435\\u0441\\u043f\\u0440\\u0430\\u0432\\u043d\\u043e\\u044e \\u0444\\u043b\\u0435\\u0448\\u0446\\u0456.\\u00a0\\u0422\\u043e\\u0434\\u0456-\\u0442\\u043e \\u0456 \\u043f\\u043e\\u0447\\u0430\\u043b\\u0438\\u0441\\u044f \\u043f\\u043e\\u0448\\u0443\\u043a\\u0438 \\u0432\\u0438\\u0440\\u0456\\u0448\\u0435\\u043d\\u043d\\u044f \\u0434\\u0430\\u043d\\u043e\\u0457 \\u043f\\u0440\\u043e\\u0431\\u043b\\u0435\\u043c\\u0438, \\u0430\\u043b\\u0435 \\u0442\\u0430\\u043a \\u0440\\u0456\\u0448\\u0435\\u043d\\u043d\\u044f \\u044f \\u0456 \\u043d\\u0435 \\u0437\\u043d\\u0430\\u0439\\u0448\\u043e\\u0432.\\u00a0\\u041f\\u043e\\u0442\\u0456\\u043c \\u044f, \\u041f\\u0430\\u0440\\u0430\\u0441\\u044e\\u043a \\u0404\\u0432\\u0433\\u0435\\u043d \\u0412\\u043e\\u043b\\u043e\\u0434\\u0438\\u043c\\u0438\\u0440\\u043e\\u0432\\u0438\\u0447 (\\u0437\\u0430\\u0441\\u043d\\u043e\\u0432\\u043d\\u0438\\u043a), \\u043f\\u043e\\u0447\\u0430\\u043b\\u0438 \\u0432\\u0438\\u0432\\u0447\\u0430\\u0442\\u0438 \\u0442\\u0435\\u043c\\u0443 \\u0432\\u0456\\u0434\\u043d\\u043e\\u0432\\u043b\\u0435\\u043d\\u043d\\u044f \\u0434\\u0430\\u043d\\u0438\\u0445 \\u0437 \\u0444\\u043b\\u0435\\u0448\\u043e\\u043a \\u0456 \\u043d\\u0430\\u0442\\u043a\\u043d\\u0443\\u0432\\u0441\\u044f \\u043d\\u0430 \\u043e\\u0431\\u043b\\u0430\\u0434\\u043d\\u0430\\u043d\\u043d\\u044f AceLab PC 3000. \\u0423 \\u0442\\u0456 \\u0447\\u0430\\u0441\\u0438 \\u043c\\u0438 \\u0437\\u0430\\u0439\\u043c\\u0430\\u043b\\u0438\\u0441\\u044f \\u0440\\u0435\\u043c\\u043e\\u043d\\u0442\\u043e\\u043c \\u043a\\u043e\\u043c\\u043f\'\\u044e\\u0442\\u0435\\u0440\\u0456\\u0432 (\\u0431\\u043b\\u043e\\u0447\\u043d\\u0438\\u0439 \\u0440\\u0435\\u043c\\u043e\\u043d\\u0442) \\u0456 \\u043f\\u043e\\u0434\\u0456\\u0431\\u043d\\u0438\\u043c\\u0438 \\u0440\\u043e\\u0431\\u043e\\u0442\\u0430\\u043c\\u0438, \\u0430\\u043b\\u0435 \\u0432\\u043e\\u0441\\u043a\\u0440\\u0435\\u0441\\u0456\\u043d\\u043d\\u044f \\u0434\\u0430\\u043d\\u0438\\u0445 \\u0442\\u043e\\u0434\\u0456 \\u0437\\u0434\\u0430\\u0432\\u0430\\u043b\\u043e\\u0441\\u044f \\u0447\\u0438\\u043c\\u043e\\u0441\\u044c \\u043a\\u043e\\u0441\\u043c\\u0456\\u0447\\u043d\\u0438\\u043c \\u0456 \\u043d\\u0435 \\u0437\\u0430\\u0437\\u043d\\u0430\\u0432\\u0448\\u0438 \\u0456 \\u043f\\u0440\\u0438\\u0442\\u044f\\u0433\\u0443\\u0432\\u0430\\u043b\\u043e, \\u044f\\u043a \\u043c\\u0430\\u0433\\u043d\\u0456\\u0442\\u043e\\u043c.\\u00a0\\u041d\\u0430\\u043a\\u043e\\u043f\\u0438\\u0447\\u0438\\u0432\\u0448\\u0438 \\u0433\\u0440\\u043e\\u0448\\u0456 \\u0456 \\u0437\\u0456\\u0431\\u0440\\u0430\\u0432\\u0448\\u0438\\u0441\\u044c \\u0437 \\u0434\\u0443\\u043c\\u043a\\u0430\\u043c\\u0438 \\u043c\\u0438 \\u0437\\u0430\\u043c\\u043e\\u0432\\u0438\\u043b\\u0438 \\u043d\\u0430\\u0448 \\u043f\\u0435\\u0440\\u0448\\u0438\\u0439 \\u043a\\u043e\\u043c\\u043f\\u043b\\u0435\\u043a\\u0441 PC 3000 Flash 2.0 (\\u0437\\u0430\\u0440\\u0430\\u0437 \\u0432\\u0436\\u0435 \\u0432\\u0435\\u0440\\u0441\\u0456\\u044f 4.0).\\u00a0\\u041d\\u0430 \\u0442\\u043e\\u0439 \\u043c\\u043e\\u043c\\u0435\\u043d\\u0442 (\\u043a\\u0456\\u043d\\u0435\\u0446\\u044c 2009 \\u0440\\u043e\\u043a\\u0443) \\u0432\\u0430\\u0440\\u0442\\u0456\\u0441\\u0442\\u044c \\u043a\\u043e\\u043c\\u043f\\u043b\\u0435\\u043a\\u0441\\u0443 \\u0441\\u0442\\u0430\\u043d\\u043e\\u0432\\u0438\\u0432 1430 \\u0443.\\u043e.\\u00a0\\u0413\\u0440\\u043e\\u0448\\u0456 \\u043d\\u0435 \\u043c\\u0430\\u043b\\u0456, \\u0430 \\u044f\\u043a\\u0449\\u043e \\u0449\\u0435 \\u0432\\u0440\\u0430\\u0445\\u0443\\u0432\\u0430\\u0442\\u0438, \\u0449\\u043e \\u0432 \\u043f\\u0430\\u0440\\u0456 \\u0434\\u043e \\u043a\\u043e\\u043c\\u043f\\u043b\\u0435\\u043a\\u0441\\u0443 \\u043f\\u043e\\u0442\\u0440\\u0456\\u0431\\u0435\\u043d \\u0431\\u0443\\u0432 \\u043f\\u043e\\u0442\\u0443\\u0436\\u043d\\u0438\\u0439 \\u041f\\u041a (\\u043d\\u0435 \\u043c\\u0435\\u043d\\u0448\\u0435 4 \\u044f\\u0434\\u0435\\u0440, 8\\u0413\\u0431 \\u043e\\u043f\\u0435\\u0440\\u0430\\u0442\\u0438\\u0432\\u043d\\u043e\\u0457 \\u043f\\u0430\\u043c\'\\u044f\\u0442\\u0456,\\u00a02 \\u0436\\u043e\\u0440\\u0441\\u0442\\u043a\\u0438\\u0445 \\u0434\\u0438\\u0441\\u043a\\u0430 \\u0432 RAID), \\u0430 \\u0442\\u0430\\u043a\\u043e\\u0436 \\u043f\\u0430\\u044f\\u043b\\u044c\\u043d\\u0435 \\u043e\\u0431\\u043b\\u0430\\u0434\\u043d\\u0430\\u043d\\u043d\\u044f \\u0456 \\u043a\\u0443\\u043f\\u0430 \\u0432\\u0441\\u044f\\u043a\\u043e\\u0457 \\u0434\\u0440\\u0456\\u0431\'\\u044f\\u0437\\u043a\\u0443.\\u00a0\\u0420\\u0430\\u0437\\u043e\\u043c \\u0432\\u0435\\u0441\\u044c \\u043d\\u0430\\u0431\\u0456\\u0440 \\u043e\\u0431\\u0456\\u0439\\u0448\\u043e\\u0432\\u0441\\u044f \\u043c\\u0430\\u0439\\u0436\\u0435 \\u0432 2,5 \\u0442\\u0438\\u0441\\u044f\\u0447\\u0456 \\u0432\\u0456\\u0447\\u043d\\u043e \\u0437\\u0435\\u043b\\u0435\\u043d\\u0438\\u0445.<\\/span><\\/p>\\r\\n<p style=\\\"text-align: justify;\\\"><span style=\\\"font-size: 14pt; font-family: tahoma, arial, helvetica, sans-serif;\\\">\\u041d\\u0430 \\u0441\\u044c\\u043e\\u0433\\u043e\\u0434\\u043d\\u0456\\u0448\\u043d\\u0456\\u0439 \\u0434\\u0435\\u043d\\u044c \\u0446\\u0435\\u043d\\u0442\\u0440 \\u0432\\u0456\\u0434\\u043d\\u043e\\u0432\\u043b\\u0435\\u043d\\u043d\\u044f \\u0434\\u0430\\u043d\\u0438\\u0445 \\u0437\\u0434\\u0456\\u0439\\u0441\\u043d\\u044e\\u0454 \\u043a\\u043e\\u043c\\u043f\\u043b\\u0435\\u043a\\u0441 \\u043f\\u043e\\u0441\\u043b\\u0443\\u0433 \\u0432 \\u0441\\u0444\\u0435\\u0440\\u0456 Data Recovery \\u0432\\u0456\\u0434 \\u043f\\u0440\\u043e\\u0441\\u0442\\u0438\\u0445 (\\u0432\\u0438\\u0434\\u0430\\u043b\\u0438\\u043b\\u0438 \\u0430\\u0431\\u043e \\u0432\\u0456\\u0434\\u0444\\u043e\\u0440\\u043c\\u0430\\u0442\\u0443\\u0432\\u0430\\u043b\\