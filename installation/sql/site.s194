30\\u043d\\u043d\\u044b\\u0435 \\u0441 \\u0444\\u043b\\u0435\\u0448\\u0435\\u043a \\u0438\\u043b\\u0438 \\u043a\\u0430\\u0440\\u0442 \\u043f\\u0430\\u043c\\u044f\\u0442\\u0438, \\u043d\\u043e \\u043f\\u043e \\u0444\\u0430\\u043a\\u0442\\u0443 \\u2013 90%, \\u0447\\u0442\\u043e \\u0444\\u0438\\u0440\\u043c\\u0430-\\u043f\\u043e\\u0441\\u0440\\u0435\\u0434\\u043d\\u0438\\u043a \\u043c\\u0435\\u0436\\u0434\\u0443 \\u043b\\u0430\\u0431\\u043e\\u0440\\u0430\\u0442\\u043e\\u0440\\u0438\\u0435\\u0439 \\u0438 \\u0412\\u0430\\u043c\\u0438 (\\u043a\\u043b\\u0438\\u0435\\u043d\\u0442\\u043e\\u043c), \\u0447\\u0442\\u043e \\u0432 \\u0438\\u0442\\u043e\\u0433\\u0435 \\u0441\\u043a\\u0430\\u0437\\u044b\\u0432\\u0430\\u0435\\u0442\\u0441\\u044f \\u043d\\u0430 \\u0441\\u043a\\u043e\\u0440\\u043e\\u0441\\u0442\\u0438 \\u0432\\u044b\\u043f\\u043e\\u043b\\u043d\\u0435\\u043d\\u0438\\u044f \\u0440\\u0430\\u0431\\u043e\\u0442 \\u0438 \\u043d\\u0430 \\u043a\\u043e\\u043d\\u0435\\u0447\\u043d\\u043e\\u0439 \\u0441\\u0442\\u043e\\u0438\\u043c\\u043e\\u0441\\u0442\\u0438. Kiev Data Recovery \\u2013 \\u044d\\u0442\\u043e \\u0438\\u043c\\u0435\\u043d\\u043d\\u043e \\u043b\\u0430\\u0431\\u043e\\u0440\\u0430\\u0442\\u043e\\u0440\\u0438\\u044f, \\u0433\\u0434\\u0435 \\u0440\\u0435\\u0430\\u043b\\u044c\\u043d\\u043e \\u0435\\u0441\\u0442\\u044c \\u043d\\u0435\\u043e\\u0431\\u0445\\u043e\\u0434\\u0438\\u043c\\u043e\\u0435 \\u043e\\u0431\\u043e\\u0440\\u0443\\u0434\\u043e\\u0432\\u0430\\u043d\\u0438\\u0435 \\u0438 \\u0440\\u0430\\u0431\\u043e\\u0442\\u0430\\u044e\\u0442 \\u043a\\u0432\\u0430\\u043b\\u0438\\u0444\\u0438\\u0446\\u0438\\u0440\\u043e\\u0432\\u0430\\u043d\\u043d\\u044b\\u0435 \\u0441\\u043f\\u0435\\u0446\\u0438\\u0430\\u043b\\u0438\\u0441\\u0442\\u044b. \\u0427\\u0430\\u0441\\u0442\\u043e \\u0431\\u044b\\u0432\\u0430\\u0435\\u0442 \\u0442\\u0430\\u043a, \\u0444\\u043b\\u0435\\u0448\\u043a\\u0430 Kingston \\u043d\\u0435 \\u0440\\u0430\\u0431\\u043e\\u0442\\u0430\\u0435\\u0442 \\u0438 \\u